{
  "name": "@pearson-components/elements-sdk",
  "version": "1.12.0",
  "description": "Styling and basic JavaScript components for UI development.\"",
  "main": "build/css/elements.css",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=development webpack-dev-server --hot",
    "gh-pages": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/gh-pages.js",
    "build": "NODE_ENV=production webpack -p",
    "test": "jest --no-cache --coverage",
    "gen-changelog": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/gen-changelog.js",
    "version": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/version.js",
    "ci": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/ci.js",
    "release": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/release.js",
    "verify": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/verify.js",
    "postpublish": "npm access public && echo 'Package scope set to public!'"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "**/src/*.{js,jsx}",
      "**/src/**/*.{js,jsx}",
      "!**/src/**/components/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|demo|coverage)/",
      "<rootDir>/src/styles"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pearson-Higher-Ed/elements-sdk.git"
  },
  "dependencies": {
    "babel-plugin-array-includes": "^2.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "input-format": "^0.1.15",
    "libphonenumber-js": "^0.4.42",
    "react-responsive-ui": "^0.10.6"
  },
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.5.6",
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.10.0",
    "colors": "~1.1.2",
    "conventional-changelog": "~1.1.0",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^1.1.4",
    "del": "^2.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "~3.16.1",
    "expect": "~1.18.0",
    "expect-jsx": "^5.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.0",
    "fs-extra": "^4.0.1",
    "highlight.js": "^8.9.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "~5.0.1",
    "image-webpack-loader": "^3.2.0",
    "intl": "~1.1.0",
    "jest": "16.0.2",
    "jsdom": "^9.12.0",
    "markdown-it": "^5.0.2",
    "node-sass": "^4.7.2",
    "prop-types": "^15.5.6",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-intl": "~2.0.1",
    "react-router-dom": "^4.0.0-beta.6",
    "react-test-renderer": "15.6.1",
    "react-transition-group": "^1.1.3",
    "sass-loader": "^6.0.0",
    "semver": "~5.1.0",
    "sinon": "^2.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "2.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.9.5"
  },
  "keywords": [
    "pearson-components"
  ],
  "author": "Pearson Design Accelerator",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pearson-Higher-Ed/elements-sdk/issues"
  },
  "homepage": "https://github.com/Pearson-Higher-Ed/elements-sdk/"
}
